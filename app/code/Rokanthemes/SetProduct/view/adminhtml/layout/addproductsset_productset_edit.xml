<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
   <update handle="editor"/>
	<body>
        <referenceContainer name="content">
            <block class="Rokanthemes\SetProduct\Block\Adminhtml\ProductSet\Edit" name="productset_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
			<block class="Rokanthemes\SetProduct\Block\Adminhtml\ProductSet\Edit\Tabs" name="setproduct_rule_tabs">
                <block class="Rokanthemes\SetProduct\Block\Adminhtml\ProductSet\Edit\Tab\Infopac" name="setproduct_tab_infopac"/>
                <action method="addTab">
                    <argument name="name" xsi:type="string">infopac</argument>
                    <argument name="block" xsi:type="string">setproduct_tab_infopac</argument>
                </action>
            </block>
		</referenceContainer>
    </body>
</page>